<Window x:Class="LabSitInSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabSitInSystem"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="SingleBorderWindow"
        Title="MainWindow" Height="1024" Width="1440">
    <Grid>
        <Rectangle HorizontalAlignment="Left" Height="1008" VerticalAlignment="Center" Width="312" Fill="#FF2E5077"/>
        <Frame x:Name="Main" Margin="312,0,0,0" NavigationUIVisibility="Hidden"/>
        <Grid Margin="0,0,1128,0" Width="312" Height="1008">
            <Rectangle x:Name="Box1" Stroke="White" RadiusX="16" RadiusY="16" Margin="42,44,44,884" StrokeThickness="1.5" Width="226" Height="80" MouseLeftButtonDown="StudentRecordsClick"/>
            <Rectangle x:Name="Box2" Stroke="White" RadiusX="16" RadiusY="16" Margin="42,146,44,782" StrokeThickness="1.5" Width="226" Height="80" MouseLeftButtonDown="StudentLogClick"/>
            <Label x:Name="StudentRecordsButton" Content="Student Records" HorizontalAlignment="Left" Margin="42,65,0,0" VerticalAlignment="Top" Height="37" Width="228" FontFamily="Inter 18pt 18pt" FontSize="18" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" MouseLeftButtonDown="StudentRecordsClick"/>
            <Label x:Name="StudentLogButton" Content="Student Log" HorizontalAlignment="Left" Margin="42,170,0,0" VerticalAlignment="Top" Height="37" Width="228" FontFamily="Inter 18pt 18pt" FontSize="18" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" MouseLeftButtonDown="StudentLogClick"/>
        </Grid>
        <Button x:Name="LogOutButton" Content="Log Out" HorizontalAlignment="Left" Margin="1225,43,0,0" VerticalAlignment="Top" Height="41" Width="172" BorderBrush="{x:Null}" Foreground="Black" FontFamily="Inter 18pt 18pt" FontSize="20" Click="LogOutButton_Click" Background="{x:Null}"/>
    </Grid>
</Window>
